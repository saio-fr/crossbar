{
	"workers": [{
		"type": "router",
		"realms": [{
			"name": "saio",
			"roles": [{
				"name": "anonymous",
				"authorizer": "fr.saio.service.authorizer.anonymous.crossbar.can"
			}, {
				"name": "registered",
				"authorizer": "fr.saio.service.authorizer.registered.crossbar.can"
			}, {
				"name": "service",
				"permissions": [{
					"uri": "*",
					"publish": true,
					"subscribe": true,
					"call": true,
					"register": true
				}]
			}]
		}],
		"transports": [{
			"type": "websocket",
			"endpoint": {
				"type": "tcp",
				"port": 8080
			},
			"options": {
				"echo_close_codereason": true,
				"open_handshake_timeout": 5000,
				"close_handshake_timeout": 1000,
				"auto_ping_interval": 10000,
				"auto_ping_timeout": 5000,
				"auto_ping_size": 4
			},
			"auth": {
				"ticket": {
					"type": "dynamic",
					"authenticator": "fr.saio.service.authenticate"
				}
			}
		}, {
			"type": "websocket",
			"endpoint": {
				"type": "tcp",
				"port": 8081
			},
			"options": {
				"echo_close_codereason": true,
				"open_handshake_timeout": 5000,
				"close_handshake_timeout": 1000,
				"auto_ping_interval": 10000,
				"auto_ping_timeout": 5000,
				"auto_ping_size": 4
			},
			"auth": {
				"wampcra": {
					"type": "static",
					"users": {
            "service": {
              "secret": "service",
              "role": "service"
            }
          }
				}
			}
		}]
	}]
}
